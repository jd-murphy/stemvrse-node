<div class="container">
    <div class="row">
        <h1>{{videoDisplayName}}</h1><strong><a class="favoriteVideo" href="#favoriteVideo' + strippedName + '" id="favoriteVideo{{videoName}}" style="float: right; margin: .5rem; padding-top: .5rem; padding-left: .5rem;"><img id="favButton{{videoName}}" class="favButton" src="/assets/favorite.png" data-video-display-name="{{videoName}}" width="24px" height="24px"></a></strong>
    </div>
</div>
       
<p>This is the page for {{videoDisplayName}}</p>


<div class="container">
    <div class="row">


        <div class="col-7">
            <div id="videoDiv" data-video-name="{{videoName}}">
                <div>
                    <iframe width="560" height="315" src="{{link}}'" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
                </div> 
            </div>
        </div>


         <div class="col-5">
            <div id="mapid"></div>
        </div>


    </div>


    <div>
        
        {{#if content}}
            {{> (this.name) }}
        {{/if}}
        
    </div>
</div>
       